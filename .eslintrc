{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true        
    },
    "rules": {
      "require-jsdoc": "off",
      "eol-last":"off",
      "object-curly-spacing":"off",
      "arrow-parens":"off",
      "comma-dangle":"off",
      "linebreak-style": "off",
       "semi": "off"
    },
    "extends": ["eslint:recommended", "google"]
}